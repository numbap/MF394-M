{
  "expo": {
    "name": "UmmYou",
    "slug": "ummyoumobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "sdkVersion": "54.0.0",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#f5f3ef"
    },
    "scheme": "com.ummyou.facememorizer",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "ios": {
      "bundleIdentifier": "com.ummyou.ios",
      "buildNumber": "1"
    },
    "android": {
      "package": "com.ummyou.android",
      "versionCode": 1
    },
    "web": {
      "favicon": "./thumbnail.png"
    },
    "plugins": [
      "expo-image-picker",
      "@react-native-google-signin/google-signin",
      "expo-web-browser"
    ],
    "extra": {
      "eas": {
        "projectId": "your-project-id"
      }
    }
  }
}
