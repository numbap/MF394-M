      "Bash(/Users/patjo/Dev/mf394-M/mf394-mobile/src/store/slices/tags.slice.ts << 'EOF'\n/**\n * Tags Redux Slice\n *\n * Manages custom tags created by users.\n * Default tags \\(from AVAILABLE_TAGS constant\\) are always available.\n * Custom tags are persisted to storage and synced to backend.\n */\n\nimport { createSlice, PayloadAction } from '@reduxjs/toolkit';\nimport type { RootState } from '../index';\nimport { AVAILABLE_TAGS } from '../../constants';\n\ninterface TagsState {\n  customTags: string[];\n  loading: boolean;\n  error: string | null;\n}\n\nconst initialState: TagsState = {\n  customTags: [],\n  loading: false,\n  error: null,\n};\n\nconst tagsSlice = createSlice\\({\n  name: 'tags',\n  initialState,\n  reducers: {\n    // Set custom tags \\(e.g., after loading from storage\\)\n    setCustomTags: \\(state, action: PayloadAction<string[]>\\) => {\n      state.customTags = action.payload;\n      state.error = null;\n    },\n\n    // Add a new custom tag\n    addCustomTag: \\(state, action: PayloadAction<string>\\) => {\n      const tagName = action.payload.trim\\(\\);\n\n      // Prevent duplicates \\(check against both custom and default tags\\)\n      const allTags = [...AVAILABLE_TAGS, ...state.customTags];\n      if \\(!allTags.includes\\(tagName\\) && tagName.length > 0\\) {\n        state.customTags.push\\(tagName\\);\n        state.error = null;\n      } else {\n        state.error = 'Tag already exists or is invalid';\n      }\n    },\n\n    // Delete a custom tag\n    deleteCustomTag: \\(state, action: PayloadAction<string>\\) => {\n      state.customTags = state.customTags.filter\\(tag => tag !== action.payload\\);\n      state.error = null;\n    },\n\n    // Clear error\n    clearTagError: \\(state\\) => {\n      state.error = null;\n    },\n\n    // Set loading state\n    setTagsLoading: \\(state, action: PayloadAction<boolean>\\) => {\n      state.loading = action.payload;\n    },\n  },\n}\\);\n\nexport const {\n  setCustomTags,\n  addCustomTag,\n  deleteCustomTag,\n  clearTagError,\n  setTagsLoading,\n} = tagsSlice.actions;\n\n// Selectors\nexport const selectCustomTags = \\(state: RootState\\) => state.tags.customTags;\nexport const selectTagsLoading = \\(state: RootState\\) => state.tags.loading;\nexport const selectTagsError = \\(state: RootState\\) => state.tags.error;\n\n/**\n * Get all available tags \\(default + custom\\)\n */\nexport const selectAllTags = \\(state: RootState\\): string[] => {\n  return [...AVAILABLE_TAGS, ...state.tags.customTags];\n};\n\n/**\n * Check if a tag is a default \\(system\\) tag\n */\nexport const isDefaultTag = \\(tagName: string\\): boolean => {\n  return AVAILABLE_TAGS.includes\\(tagName\\);\n};\n\nexport default tagsSlice.reducer;\nEOF)",
      "Bash(/Users/patjo/Dev/mf394-M/mf394-mobile/src/components/TagEditorOverlay/TagEditorOverlay.tsx << 'EOF'\n/**\n * TagEditorOverlay Component\n *\n * Full-screen overlay for managing tags \\(add/delete\\).\n * Works globally across Add, Edit, and Party modes.\n *\n * Features:\n * - Add new custom tags\n * - Delete custom tags\n * - Default tags cannot be deleted\n * - Global changes affect all contacts\n */\n\nimport React, { useState, useEffect } from 'react';\nimport {\n  View,\n  Text,\n  TextInput,\n  ScrollView,\n  TouchableOpacity,\n  Modal,\n  StyleSheet,\n  Alert,\n} from 'react-native';\nimport { useDispatch, useSelector } from 'react-redux';\nimport { FontAwesome } from '@expo/vector-icons';\nimport { colors, spacing, radii, typography } from '../../theme/theme';\nimport { FormButtons } from '../FormButtons';\nimport {\n  selectAllTags,\n  selectCustomTags,\n  addCustomTag,\n  deleteCustomTag,\n  clearTagError,\n  isDefaultTag,\n} from '../../store/slices/tags.slice';\nimport { updateContact } from '../../store/slices/contacts.slice';\nimport type { RootState } from '../../store';\n\ninterface TagEditorOverlayProps {\n  visible: boolean;\n  selectedTags?: string[];\n  onClose: \\(\\) => void;\n  onSave?: \\(selectedTags: string[]\\) => void;\n}\n\nexport function TagEditorOverlay\\({\n  visible,\n  selectedTags = [],\n  onClose,\n  onSave,\n}: TagEditorOverlayProps\\) {\n  const dispatch = useDispatch\\(\\);\n  const allTags = useSelector\\(selectAllTags\\);\n  const customTags = useSelector\\(selectCustomTags\\);\n  const [newTagName, setNewTagName] = useState\\(''\\);\n  const [localSelectedTags, setLocalSelectedTags] = useState<string[]>\\(selectedTags\\);\n\n  // Sync with prop changes\n  useEffect\\(\\(\\) => {\n    setLocalSelectedTags\\(selectedTags\\);\n  }, [selectedTags]\\);\n\n  const handleAddTag = \\(\\) => {\n    const trimmedName = newTagName.trim\\(\\);\n\n    if \\(!trimmedName\\) {\n      Alert.alert\\('Invalid Tag', 'Tag name cannot be empty.'\\);\n      return;\n    }\n\n    if \\(allTags.includes\\(trimmedName\\)\\) {\n      Alert.alert\\('Duplicate Tag', 'This tag already exists.'\\);\n      return;\n    }\n\n    dispatch\\(addCustomTag\\(trimmedName\\)\\);\n    setNewTagName\\(''\\);\n  };\n\n  const handleDeleteTag = \\(tagName: string\\) => {\n    if \\(isDefaultTag\\(tagName\\)\\) {\n      Alert.alert\\('Cannot Delete', 'Default tags cannot be deleted.'\\);\n      return;\n    }\n\n    Alert.alert\\(\n      'Delete Tag',\n      `Are you sure you want to delete \"${tagName}\"? This will remove it from all contacts.`,\n      [\n        { text: 'Cancel', style: 'cancel' },\n        {\n          text: 'Delete',\n          style: 'destructive',\n          onPress: \\(\\) => {\n            dispatch\\(deleteCustomTag\\(tagName\\)\\);\n\n            // Remove from locally selected tags if present\n            setLocalSelectedTags\\(\\(prev\\) => prev.filter\\(\\(t\\) => t !== tagName\\)\\);\n\n            // TODO: Remove from all contacts \\(handled by parent or middleware\\)\n            // This would require iterating through all contacts and updating them\n          },\n        },\n      ]\n    \\);\n  };\n\n  const handleToggleTag = \\(tagName: string\\) => {\n    setLocalSelectedTags\\(\\(prev\\) => {\n      if \\(prev.includes\\(tagName\\)\\) {\n        return prev.filter\\(\\(t\\) => t !== tagName\\);\n      } else {\n        return [...prev, tagName];\n      }\n    }\\);\n  };\n\n  const handleSave = \\(\\) => {\n    if \\(onSave\\) {\n      onSave\\(localSelectedTags\\);\n    }\n    dispatch\\(clearTagError\\(\\)\\);\n    onClose\\(\\);\n  };\n\n  const handleCancel = \\(\\) => {\n    setLocalSelectedTags\\(selectedTags\\); // Reset to original\n    setNewTagName\\(''\\);\n    dispatch\\(clearTagError\\(\\)\\);\n    onClose\\(\\);\n  };\n\n  return \\(\n    <Modal\n      visible={visible}\n      animationType=\"slide\"\n      transparent={false}\n      onRequestClose={handleCancel}\n    >\n      <View style={styles.container}>\n        {/* Header */}\n        <View style={styles.header}>\n          <Text style={styles.title}>Manage Tags</Text>\n          <TouchableOpacity\n            onPress={handleCancel}\n            style={styles.closeButton}\n            testID=\"close-button\"\n          >\n            <FontAwesome name=\"times\" size={24} color={colors.semantic.text} />\n          </TouchableOpacity>\n        </View>\n\n        <ScrollView style={styles.content} showsVerticalScrollIndicator={false}>\n          {/* Available Tags Section */}\n          <View style={styles.section}>\n            <Text style={styles.sectionTitle}>Available Tags</Text>\n            <View style={styles.tagsList}>\n              {allTags.map\\(\\(tag\\) => {\n                const isDefault = isDefaultTag\\(tag\\);\n                const isSelected = localSelectedTags.includes\\(tag\\);\n\n                return \\(\n                  <View key={tag} style={styles.tagRow}>\n                    {/* Tag Selection Checkbox */}\n                    <TouchableOpacity\n                      style={styles.tagCheckbox}\n                      onPress={\\(\\) => handleToggleTag\\(tag\\)}\n                      testID={`tag-checkbox-${tag}`}\n                    >\n                      <View\n                        style={[\n                          styles.checkbox,\n                          isSelected && styles.checkboxSelected,\n                        ]}\n                      >\n                        {isSelected && \\(\n                          <FontAwesome\n                            name=\"check\"\n                            size={16}\n                            color={colors.semantic.background}\n                          />\n                        \\)}\n                      </View>\n                      <Text style={styles.tagName}>{tag}</Text>\n                      <View\n                        style={[\n                          styles.badge,\n                          isDefault ? styles.badgeDefault : styles.badgeCustom,\n                        ]}\n                      >\n                        <Text\n                          style={[\n                            styles.badgeText,\n                            isDefault ? styles.badgeTextDefault : styles.badgeTextCustom,\n                          ]}\n                        >\n                          {isDefault ? 'Default' : 'Custom'}\n                        </Text>\n                      </View>\n                    </TouchableOpacity>\n\n                    {/* Delete Button \\(Custom tags only\\) */}\n                    {!isDefault && \\(\n                      <TouchableOpacity\n                        onPress={\\(\\) => handleDeleteTag\\(tag\\)}\n                        style={styles.deleteButton}\n                        testID={`delete-tag-${tag}`}\n                      >\n                        <FontAwesome\n                          name=\"trash\"\n                          size={18}\n                          color={colors.semantic.error}\n                        />\n                      </TouchableOpacity>\n                    \\)}\n                  </View>\n                \\);\n              }\\)}\n            </View>\n          </View>\n\n          {/* Add New Tag Section */}\n          <View style={styles.section}>\n            <Text style={styles.sectionTitle}>Add New Tag</Text>\n            <View style={styles.addTagContainer}>\n              <TextInput\n                style={styles.input}\n                placeholder=\"Enter tag name...\"\n                placeholderTextColor={colors.semantic.textTertiary}\n                value={newTagName}\n                onChangeText={setNewTagName}\n                onSubmitEditing={handleAddTag}\n                returnKeyType=\"done\"\n                testID=\"new-tag-input\"\n              />\n              <TouchableOpacity\n                style={[styles.addButton, !newTagName.trim\\(\\) && styles.addButtonDisabled]}\n                onPress={handleAddTag}\n                disabled={!newTagName.trim\\(\\)}\n                testID=\"add-tag-button\"\n              >\n                <FontAwesome\n                  name=\"plus\"\n                  size={16}\n                  color={colors.semantic.background}\n                />\n                <Text style={styles.addButtonText}>Add Tag</Text>\n              </TouchableOpacity>\n            </View>\n          </View>\n        </ScrollView>\n\n        {/* Footer Buttons */}\n        <View style={styles.footer}>\n          <FormButtons\n            primaryButton={{\n              label: 'Save',\n              icon: 'check',\n              onPress: handleSave,\n            }}\n            cancelButton={{\n              label: 'Cancel',\n              icon: 'times',\n              onPress: handleCancel,\n            }}\n          />\n        </View>\n      </View>\n    </Modal>\n  \\);\n}\n\nconst styles = StyleSheet.create\\({\n  container: {\n    flex: 1,\n    backgroundColor: colors.semantic.background,\n  },\n  header: {\n    flexDirection: 'row',\n    alignItems: 'center',\n    justifyContent: 'space-between',\n    paddingHorizontal: spacing.lg,\n    paddingVertical: spacing.md,\n    borderBottomWidth: 1,\n    borderBottomColor: colors.semantic.border,\n  },\n  title: {\n    fontSize: typography.headline.large.fontSize,\n    fontWeight: '700',\n    color: colors.semantic.text,\n  },\n  closeButton: {\n    padding: spacing.sm,\n  },\n  content: {\n    flex: 1,\n    padding: spacing.lg,\n  },\n  section: {\n    marginBottom: spacing.xl,\n  },\n  sectionTitle: {\n    fontSize: typography.headline.small.fontSize,\n    fontWeight: '600',\n    color: colors.semantic.text,\n    marginBottom: spacing.md,\n  },\n  tagsList: {\n    gap: spacing.sm,\n  },\n  tagRow: {\n    flexDirection: 'row',\n    alignItems: 'center',\n    justifyContent: 'space-between',\n    paddingVertical: spacing.sm,\n    paddingHorizontal: spacing.md,\n    backgroundColor: colors.semantic.surfaceSecondary,\n    borderRadius: radii.md,\n  },\n  tagCheckbox: {\n    flexDirection: 'row',\n    alignItems: 'center',\n    flex: 1,\n    gap: spacing.md,\n  },\n  checkbox: {\n    width: 24,\n    height: 24,\n    borderRadius: radii.sm,\n    borderWidth: 2,\n    borderColor: colors.semantic.border,\n    alignItems: 'center',\n    justifyContent: 'center',\n  },\n  checkboxSelected: {\n    backgroundColor: colors.primary.main,\n    borderColor: colors.primary.main,\n  },\n  tagName: {\n    flex: 1,\n    fontSize: typography.body.medium.fontSize,\n    color: colors.semantic.text,\n  },\n  badge: {\n    paddingHorizontal: spacing.sm,\n    paddingVertical: spacing.xs,\n    borderRadius: radii.sm,\n  },\n  badgeDefault: {\n    backgroundColor: colors.primary.light,\n  },\n  badgeCustom: {\n    backgroundColor: colors.secondary.light,\n  },\n  badgeText: {\n    fontSize: typography.body.small.fontSize,\n    fontWeight: '600',\n  },\n  badgeTextDefault: {\n    color: colors.primary.dark,\n  },\n  badgeTextCustom: {\n    color: colors.secondary.dark,\n  },\n  deleteButton: {\n    padding: spacing.sm,\n    marginLeft: spacing.sm,\n  },\n  addTagContainer: {\n    gap: spacing.md,\n  },\n  input: {\n    height: 48,\n    borderWidth: 1,\n    borderColor: colors.semantic.border,\n    borderRadius: radii.md,\n    paddingHorizontal: spacing.md,\n    fontSize: typography.body.medium.fontSize,\n    color: colors.semantic.text,\n    backgroundColor: colors.semantic.surface,\n  },\n  addButton: {\n    flexDirection: 'row',\n    alignItems: 'center',\n    justifyContent: 'center',\n    gap: spacing.sm,\n    backgroundColor: colors.primary.main,\n    paddingVertical: spacing.md,\n    paddingHorizontal: spacing.lg,\n    borderRadius: radii.md,\n  },\n  addButtonDisabled: {\n    backgroundColor: colors.semantic.border,\n  },\n  addButtonText: {\n    fontSize: typography.body.medium.fontSize,\n    fontWeight: '600',\n    color: colors.semantic.background,\n  },\n  footer: {\n    paddingHorizontal: spacing.lg,\n    paddingVertical: spacing.md,\n    borderTopWidth: 1,\n    borderTopColor: colors.semantic.border,\n  },\n}\\);\nEOF)",
