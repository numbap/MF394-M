{
  "expo": {
    "name": "Face Memorizer",
    "slug": "face-memorizer",
    "version": "1.0.0",
    "orientation": "portrait",
    "sdkVersion": "54.0.0",
    "icon": "./assets/icon.png",
    "scheme": "com.ummyou.facememorizer",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "ios": {
      "supportsTabletMode": true,
      "bundleIdentifier": "com.ummyou.ios",
      "buildNumber": "1",
      "requireFullScreen": true,
      "deploymentTarget": "15.1"
    },
    "android": {
      "package": "com.ummyou.android",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-image-picker"
    ],
    "extra": {
      "eas": {
        "projectId": "your-project-id"
      }
    }
  }
}